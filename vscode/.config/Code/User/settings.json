{
  "terminal.integrated.shell.linux": "/usr/bin/fish",
  "editor.fontFamily": "'FuraCode Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.cursorStyle": "line-thin",
  "editor.fontLigatures": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontFamily": "'FuraCode Nerd Font Mono'",
  "terminal.external.linuxExec": "kitty",
  "window.zoomLevel": 0,
  "explorer.confirmDragAndDrop": false,
  "editor.formatOnSave": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "vsicons.dontShowNewVersionMessage": true,
  "workbench.activityBar.visible": false,
  "editor.minimap.enabled": false,
  "editor.highlightActiveIndentGuide": false,
  "editor.renderIndentGuides": false,
  "zenMode.fullScreen": false,
  "window.menuBarVisibility": "toggle",
  "workbench.colorCustomizations": {
    "editorGroup.border": "#1e1e1e",
    "panel.border": "#1e1e1e",
    "editorOverviewRuler.border": "#1e1e1e"
  },
  "workbench.colorTheme": "Monokai Dimmed"
}
